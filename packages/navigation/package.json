{
  "name": "navigation",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve  --mode development --config config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --mode production --config config/webpack.prod.js",
    "build:dev": "cross-env NODE_ENV=development webpack --mode development --config ./config/webpack.prod.js",
    "serve": "serve dist -l 3001",
    "clean": "rm -rf dist",
    "test": "jest src --passWithNoTests --maxWorkers=50%",
    "test:coverage": "jest src --passWithNoTests --maxWorkers=50% --coverage",
    "lint": "eslint src --ext js,ts,tsx",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "upgrade:packages": "npx npm-check-updates -u --packageFile package.json"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "redux-micro-frontend": "^1.2.0",
    "single-spa": "^5.9.3",
    "single-spa-react": "^4.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.14",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "babel-loader": "^8.2.3",
    "bundle-loader": "^0.5.6",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^15.0.0",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-ts-react-important-stuff": "^3.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "prettier-quick": "^0.0.5",
    "serve": "^13.0.2",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2",
    "webpack-merge": "^5.8.0"
  }
}
