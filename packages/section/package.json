{
  "name": "section",
  "version": "1.0.0",
  "description": "Base micro-frontend application",
  "main": "index.js",
  "keywords": [
    "Webpack",
    "Module",
    "Federation",
    "javascript",
    "typescript"
  ],
  "config": {
    "analyze": false
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.production.js",
    "build:dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack.production.js",
    "serve": "npx serve dist -l 3003",
    "check-types": "tsc",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "precise-commits": "precise-commits",
    "pretest": "yarn run test:clean && yarn run lint",
    "format": "prettier . --write",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack/webpack.development.js",
    "test:clean": "rimraf ./coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "test": "cross-env NODE_ENV=test jest --config jest.config.js --coverage",
    "upgrade:packages": "npx npm-check-updates -u --packageFile package.json"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "MIT",
  "engines": {
    "npm": ">=6.14.6",
    "node": ">=12.18.3"
  },
  "dependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "single-spa-react": "^4.6.0",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-merge": "^5.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.7",
    "@babel/plugin-proposal-function-sent": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-modules-commonjs": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/enzyme": "^3.10.11",
