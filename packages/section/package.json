{
  "name": "section",
  "version": "1.0.0",
  "description": "Base micro-frontend application",
  "main": "index.js",
  "keywords": [
    "Webpack",
    "Module",
    "Federation",
    "javascript",
    "typescript"
  ],
  "config": {
    "analyze": false
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.production.js",
    "build:dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack.production.js",
    "serve": "npx serve dist -l 3003",
    "check-types": "tsc",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "precise-commits": "precise-commits",
    "pretest": "yarn run test:clean && yarn run lint",
    "format": "prettier . --write",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack/webpack.development.js",
    "test:clean": "rimraf ./coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "test": "cross-env NODE_ENV=test jest --config jest.config.js --coverage",
    "upgrade:packages": "npx npm-check-updates -u --packageFile package.json"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "MIT",
  "engines": {
    "npm": ">=6.14.6",
    "node": ">=12.18.3"
  },
  "dependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "single-spa-react": "^5.0.0",
    "typescript": "^5.6.3",
    "webpack": "^5.95.0",
    "webpack-merge": "^6.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-class-properties": "^7.18.6",
    "@babel/plugin-transform-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-function-sent": "^7.25.9",
    "@babel/plugin-transform-object-rest-spread": "^7.20.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-modules-commonjs": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@types/enzyme": "^3.10.18",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.8.4",
