    "babel-preset-react-app": "^10.0.1",
    "circular-dependency-plugin": "^5.2.2",
    "compression-webpack-plugin": "^9.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "dotenv": "^10.0.0",
